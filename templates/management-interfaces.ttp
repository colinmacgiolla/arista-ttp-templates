<group name="management_interfaces.{{management_interface}}">
interface Management{{management_interface | ORPHRASE}}
   description {{description | ORPHRASE}}
   vrf {{vrf}}
   vrf forwarding {{vrf}}
   ip address {{ip_address}}
   ipv6 enable {{ ipv6_enable | set(true) }}
   ipv6 address {{ipv6_address}}
   shutdown {{ shutdown | set(True) }}
! {{_end_}}
</group>